<!DOCTYPE html>
<html>
<head>
  <% include ../partials/_head.ejs %>
</head>
<body>
  <header>
    <% include ../partials/_nav.ejs %>
  </header>
  <main class="container map-thumbs">
    <% maps.forEach(map => { %>
      <div class="map-thumbnail thumbnail col-xs-6 col-md-4 col-lg-3">
        <a class="map-thumbnail-a" href="maps/<%= map.id %>">
          <img class="map-thumbnail-img" alt="<%= map.title %>" src="http://fillmurray.com/640/640">
          <h1 class="caption map-thumbnail-name"><%= map.title %></h1>
        </a>
      </div>
    <% }) %>
  </main>
  <% include ../partials/_scripts.ejs %>
  <% include ../partials/_gmaps_api.ejs %>
</body>
</html>
